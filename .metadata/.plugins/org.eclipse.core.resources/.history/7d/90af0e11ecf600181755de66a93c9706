
public class Arrays {
	// Attribute
	private int zahl0 = 1;
	private int zahl1 = 5;
	private int zahl2 = 10;
	private int zahl3 = 3;
	private int zahl4 = 2;
	
	private int[] zahl = {1, 5, 10, 3, 2, 645, 7, 9, 1, 2, 3};
	// Methoden
	public void gebeZahlen() {
		System.out.println(zahl0 + " " +zahl1 + " " + zahl2 + " " + zahl3  + " " + zahl4);
		int i = 4;
		System.out.println(zahl[i] + " " + zahl[1]);
		// for each
		// Für jedes
		for(int element : zahl) {
			System.out.print(element + " ");
		}
		System.out.println();
		System.out.println(zahl.length);
		//
		int j = 0;
		do {
			System.out.print(zahl[j] + " ");
		} while(j < zahl.length);
	}
}
